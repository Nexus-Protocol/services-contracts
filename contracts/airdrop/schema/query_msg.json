{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "anyOf": [
    {
      "type": "string",
      "enum": [
        "config",
        "latest_stage"
      ]
    },
    {
      "type": "object",
      "required": [
        "merkle_root"
      ],
      "properties": {
        "merkle_root": {
          "type": "object",
          "required": [
            "stage"
          ],
          "properties": {
            "stage": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "is_claimed"
      ],
      "properties": {
        "is_claimed": {
          "type": "object",
          "required": [
            "address",
            "stage"
          ],
          "properties": {
            "address": {
              "type": "string"
            },
            "stage": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
